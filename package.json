{
  "name": "team",
  "displayName": "Visual Studio Team Services",
  "description": "Connect to Team Services, monitor your builds and manage your pull requests and work items for your source repositories",
  "version": "1.108.0",
  "publisher": "ms-vsts",
  "icon": "assets/team.png",
  "markdown": "standard",
  "galleryBanner": {
    "color": "#65217C",
    "theme": "dark"
  },
  "keywords": [
    "Team Foundation Server",
    "Team Services",
    "Visual Studio Online",
    "Visual Studio Team Services",
    "VSTS"
  ],
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Microsoft/vsts-vscode.git"
  },
  "bugs": "https://github.com/Microsoft/vsts-vscode/issues",
  "license": "SEE LICENSE IN LICENSE.txt",
  "homepage": "https://github.com/Microsoft/vsts-vscode/blob/master/README.md",
  "engines": {
    "vscode": "^1.7.0"
  },
  "activationEvents": [
    "*"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Team Services extension options",
      "properties": {
        "team.appInsights.enabled": {
          "type": "boolean",
          "default": true,
          "description": "Enables Application Insights telemetry collection for the Team Services extension."
        },
        "team.logging.level": {
          "type": "string",
          "default": "",
          "description": "Set the logging level for the extension (error, warn, info, verbose, debug)."
        },
        "team.pollingInterval": {
          "type": "number",
          "default": 5,
          "description": "Specify the number of minutes to wait when polling for new builds and pull requests."
        },
        "team.remoteUrl": {
          "type": "string",
          "default": "",
          "description": "Specify the url to a project collection to use when your source code repository is not hosted with Microsoft. Requires team.teamProject."
        },
        "team.teamProject": {
          "type": "string",
          "default": "",
          "description": "Specify the team project to use when your source code repository is not hosted with Microsoft. Requires team.remoteUrl."
        },
        "team.buildDefinitionId": {
          "type": "number",
          "default": 0,
          "description": "Specify the team project's build definition Id to monitor when your source code repository is not hosted with Microsoft. Requires both team.remoteUrl and team.teamProject."
        },
        "tfvc.location": {
          "type": "string",
          "default": "",
          "description": "Specify the full path to the TF executable or script to use for TFVC functionality."
        }
      }
    },
    "commands": [
      {
        "command": "team.OpenNewTask",
        "title": "team create task"
      },
      {
        "command": "team.OpenNewBug",
        "title": "team create bug"
      },
      {
        "command": "team.OpenFileHistory",
        "title": "team view history"
      },
      {
        "command": "team.OpenBlamePage",
        "title": "team view blame"
      },
      {
        "command": "team.OpenNewPullRequest",
        "title": "team create pull request"
      },
      {
        "command": "team.OpenNewWorkItem",
        "title": "team create work item"
      },
      {
        "command": "team.GetPullRequests",
        "title": "team view pull requests"
      },
      {
        "command": "team.OpenBuildSummaryPage",
        "title": "team view build summary"
      },
      {
        "command": "team.OpenTeamSite",
        "title": "team view website"
      },
      {
        "command": "team.ViewWorkItems",
        "title": "team view work items"
      },
      {
        "command": "team.ViewWorkItemQueries",
        "title": "team view work item queries"
      },
      {
        "command": "team.SendFeedback",
        "title": "team send feedback"
      },
      {
        "command": "team.Login",
        "title": "team login"
      },
      {
        "command": "team.Logout",
        "title": "team logout"
      },
      {
        "command": "tfvc.Status",
        "title": "TFVC: Status"
      }
    ]
  },
  "scripts": {
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "chai": "^3.4.1",
    "del": "^2.2.0",
    "gulp": "^3.9.0",
    "gulp-istanbul": "^1.1.1",
    "gulp-mocha": "^3.0.1",
    "gulp-sourcemaps": "^2.4.0",
    "gulp-tslint": "^3.2.0",
    "gulp-typescript": "^3.1.4",
    "gulp-util": "^3.0.7",
    "mocha-junit-reporter": "^1.12.0",
    "should": "^8.1.1",
    "tslint": "^2.5.0",
    "typescript": "^2.1.5",
    "vscode": "^1.0.0",
    "vsts-task-lib": "^0.9.18",
    "yargs": "^5.0.0",
    "@types/applicationinsights": "0.15.33",
    "@types/chai": "^3.4.1",
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.40",
    "@types/node-uuid": "^6.0.26-alpha",
    "@types/q": "^0.0.32",
    "@types/winston": "^2.1.1",
    "@types/xml2js": "^0.0.32"
  },
  "dependencies": {
    "applicationinsights": "0.15.16",
    "event-stream": "^3.3.2",
    "fs": "0.0.2",
    "git-repo-info": "^1.1.2",
    "node-uuid": "^1.4.7",
    "open": "0.0.5",
    "opener": "^1.4.1",
    "parse-git-config": "^0.3.1",
    "path": "^0.12.7",
    "readable-stream": "^2.1.4",
    "underscore": "^1.8.3",
    "url": "^0.11.0",
    "vso-node-api": "^5.1.1",
    "winston": "^2.1.1",
    "xml2js": "^0.4.17"
  }
}
